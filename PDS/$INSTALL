//*
//* COPY REXX TO ISPEXEC
//*
//DUMP1   EXEC PGM=IEBCOPY,REGION=2M
//SYSPRINT DD  SYSOUT=*
//SYSABEND DD  SYSOUT=*
//NPDSFAST DD  DUMMY
//COMP1    DD  DISP=SHR,DSN=HLQ.CUT.CNTL
//COMP2    DD  DISP=SHR,DSN=HLQ.ISPEXEC
//SYSIN    DD  *
  COPY    O=COMP2,I=((COMP1,R))
  S M=SHOWCUT,ZCUT,ZPASTE
/*
//*
//* COPY PANELS TO ISPPLIB
//*
//DUMP1   EXEC PGM=IEBCOPY,REGION=2M
//SYSPRINT DD  SYSOUT=*
//SYSABEND DD  SYSOUT=*
//NPDSFAST DD  DUMMY
//COMP1    DD  DISP=SHR,DSN=HLQ.CUT.CNTL
//COMP2    DD  DISP=SHR,DSN=HLQ.ISPPLIB
//SYSIN    DD  *
 COPY    O=COMP2,I=((COMP1,R))
 S M=CUTDEF,CUTDEF@,CUTHELP,CUTPROM,PASTEDEF,PASTEPRM,SHOWCUTP,SHOWCUTT
